@model List<Entities.Concrete.Course>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LoginLayout.cshtml";
}



<table class="table table-dark mt-5">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Ders Adı</th>
           

        </tr>
    </thead>
    <tbody>
      
            @foreach (var x in Model)
            {
                <tr>
                    <th scope="row">@x.CourseId</th>
                    <td>@x.Title</td>
             
                   
                </tr>

            }
        
       
        

    </tbody>
</table>
