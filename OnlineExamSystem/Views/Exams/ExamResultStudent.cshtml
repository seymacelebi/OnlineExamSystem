@model List<Entities.Concrete.Course>


@{
}
<h4>Sınav Sonuçları</h4>
<table class="table">
    <thead>
        <tr>

            <th scope="col">Ders Adı</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                
                <td> <a href="@this.Url.Action("ExamResultStudentScore", "Exams" , new{@CourseId = @item.CourseId})" >@item.Title </a></td>

            </tr>
        }


    </tbody>
</table>